{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "lib": [
      "dom",
      "es2015",
      "es2015.core",
      "es6"
    ],
    "strict": true,
    "outFile": "./dist/main.bundle.js",
    "baseUrl": "./",
    "paths": {
      "@/*": [
